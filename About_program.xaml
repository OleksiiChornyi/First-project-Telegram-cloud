<Window x:Class="Telegram_cloud.About_program"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Telegram_cloud"
        mc:Ignorable="d"
        Title="About program" Height="450" Width="800">
    <Grid>
        <ScrollViewer x:Name="Scroll_viewer" VerticalScrollBarVisibility="Auto">
            <TextBlock x:Name="About_program_text" Margin="10" FontFamily="Times New Roman" FontSize="14" TextWrapping="Wrap"></TextBlock>
        </ScrollViewer>
    </Grid>
</Window>
